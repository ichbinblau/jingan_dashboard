<%= semantic_bootstrap_nested_form_for @kf_useraction_history , :url => url_for( :page=>params[:page], :action => params[:action]=="edit" ? "update" : "create") do |f| %>
  <% if @kf_useraction_history.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@kf_useraction_history.errors.count, "error") %> prohibited this kf_useraction_history from being saved:</h2>

      <ul>
      <% @kf_useraction_history.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  <%= f.inputs do %>
    <%= f.input :kf_course_id %>
    <%= f.input :kf_course_index_id %>
    <%= f.input :kf_course_item_type_id %>
    <%= f.input :action_type %>
    <%= f.input :action %>
  <% end %>
  <%= f.actions do %>
    <%= f.action :submit ,:label => params[:action]=="edit" ? "修改" : "添加" %>
  <% end %>
<% end %>
