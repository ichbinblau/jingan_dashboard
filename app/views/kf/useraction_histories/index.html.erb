<h1>
  <%= Kf::UseractionHistory.model_name.human %>管理 
  <%= link_to "新建#{ Kf::UseractionHistory.model_name.human }",  { :action => "new", :page => params[:page]} ,:class =>"btn btn-primary"%>
  <span><form action="<%= url_for%>">
    <input name="search" id="search_list_input" value="<%= params[:search]%>" type="text" />
    <input name="page"  type="hidden" value="<%= params[:page]%>"/>
    <input name="per_page" type="hidden" value="<%= params[:per_page]%>"/>
    <button href="javascript:;" onclick="search_list" class="btn" >搜索</button>
  </form></span>
</h1>
<%= will_paginate @kf_useraction_histories , renderer: BootstrapPagination::Rails %>
<table class="table table-bordered table-hover">
  <tr>
    <th width="30">ID</th>
    <th><%= Kf::UseractionHistory.human_attribute_name("kf_course_id") %></th>
    <th><%= Kf::UseractionHistory.human_attribute_name("kf_course_index_id") %></th>
    <th><%= Kf::UseractionHistory.human_attribute_name("kf_course_item_type_id") %></th>
    <th><%= Kf::UseractionHistory.human_attribute_name("action_type") %></th>
    <th><%= Kf::UseractionHistory.human_attribute_name("action") %></th>
    <th width="90">操作</th>
  </tr>

<% @kf_useraction_histories.each do |kf_useraction_history| %>
  <tr>
    <td><%= kf_useraction_history.id %></td>
    <td><%= kf_useraction_history.kf_course_id %></td>
    <td><%= kf_useraction_history.kf_course_index_id %></td>
    <td><%= kf_useraction_history.kf_course_item_type_id %></td>
    <td><%= kf_useraction_history.action_type %></td>
    <td><%= kf_useraction_history.action %></td>
    <td>
        <%= link_to '编辑',  :action => "edit" , :id =>kf_useraction_history.id , :page => params[:page]%>
        <br />
        <%= link_to '删除', { :action => "destroy", :id =>kf_useraction_history.id , :page => params[:page]},  method: :delete, data: { confirm: '确定删除？' } %>
    </td>
  </tr>
<% end %>
</table>
<%= will_paginate @kf_useraction_histories , renderer: BootstrapPagination::Rails %>
