<h1>
  <%= Kf::Doctor.model_name.human %>管理 
  <%= link_to "新建#{ Kf::Doctor.model_name.human }",  { :action => "new", :page => params[:page]} ,:class =>"btn btn-primary"%>
  <span><form action="<%= url_for%>">
    <input name="search" id="search_list_input" value="<%= params[:search]%>" type="text" />
    <input name="page"  type="hidden" value="<%= params[:page]%>"/>
    <input name="per_page" type="hidden" value="<%= params[:per_page]%>"/>
    <button href="javascript:;" onclick="search_list" class="btn" >搜索</button>
  </form></span>
</h1>
<%= will_paginate @kf_doctors , renderer: BootstrapPagination::Rails %>
<table class="table table-bordered table-hover">
  <tr>
    <th width="30">ID</th>
    <th><%= Kf::Doctor.human_attribute_name("user_info_id") %></th>
    <th><%= Kf::Doctor.human_attribute_name("hospital_id") %></th>
    <th><%= Kf::Doctor.human_attribute_name("office_id") %></th>
    <th><%= Kf::Doctor.human_attribute_name("doctor_group_id") %></th>
    <th><%= Kf::Doctor.human_attribute_name("name") %></th>
    <th><%= Kf::Doctor.human_attribute_name("image_cover") %></th>
    <th><%= Kf::Doctor.human_attribute_name("description") %></th>
    <th width="90">操作</th>
  </tr>

<% @kf_doctors.each do |kf_doctor| %>
  <tr>
    <td><%= kf_doctor.id %></td>
    <td><%= kf_doctor.user_info_id %></td>
    <td><%= kf_doctor.hospital_id %></td>
    <td><%= kf_doctor.office_id %></td>
    <td><%= kf_doctor.doctor_group_id %></td>
    <td><%= kf_doctor.name %></td>
    <td><%= kf_doctor.image_cover %></td>
    <td><%= kf_doctor.description %></td>
    <td>
        <%= link_to '编辑',  :action => "edit" , :id =>kf_doctor.id , :page => params[:page]%>
        <br />
        <%= link_to '删除', { :action => "destroy", :id =>kf_doctor.id , :page => params[:page]},  method: :delete, data: { confirm: '确定删除？' } %>
    </td>
  </tr>
<% end %>
</table>
<%= will_paginate @kf_doctors , renderer: BootstrapPagination::Rails %>
